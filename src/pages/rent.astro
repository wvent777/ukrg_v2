---
import Layout from "../layouts/Layout.astro";
import { Navbar } from "../components/Navbar.jsx";
import { Divider } from "../components/Divider";
import { Footer } from "../components/Footer.jsx";
import { RentWidget } from "../components/RentWidget";
import "../styles/Navbar.styles.css";
import "../styles/Hero.styles.css";
import "../styles/Utils.styles.css";
import "../styles/FeaturesDiagonal.styles.css";
import { ScrollUpButton } from "../components/ScrollUpButton";

const rent = {
  title: "Rent",
  aboutTitle: "Info on renting",
  about: "About...",
};
---

<Layout title="Ukpasia">
  <Navbar client:load />
  <Divider />
  <RentWidget client:load info={rent} />
  <Footer />
  <ScrollUpButton client:load />
</Layout>
